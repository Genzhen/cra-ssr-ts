{
  "apps": [
    {
      "name": "cbet-nodejs",
      "cwd": "build",
      "script": "server.js",
      "instances": "2",
      "exec_mode": "cluster",
      "env": {
        "NODE_ENV": "production",
        "PORT": 9998
      },
      "log_file": "/usr/local/project/nodejs/logs/cra-ssr-ts.access.log",
      "error_file": "/usr/local/project/nodejs/logs/cra-ssr-ts.error.log",
      "log_date_format": "YYYY-MM-DD_HH:mm Z",
      "merge_logs": true
    }
  ]
}
