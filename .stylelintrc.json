{
  "ignoreFiles": [" *.ts", " *.tsx"],
  "extends": "stylelint-config-standard",
  "rules": {
    "unit-case": null,
    "block-no-empty": null,
    "no-descending-specificity": [true, {"severity": "warning"}],
    "no-empty-source": [true, {"severity": "warning"}],
    "selector-type-no-unknown": [true, {"ignoreTypes": ["page"]}]
  }
}
